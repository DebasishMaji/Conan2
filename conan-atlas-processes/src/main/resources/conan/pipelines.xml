<?xml version="1.0" encoding="UTF-8"?>

<conan-pipelines xmlns="http://www.ebi.ac.uk/arrayexpress/conan/pipelines"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://www.ebi.ac.uk/arrayexpress/conan/pipelines
                                     http://www.ebi.ac.uk/arrayexpress/conan/pipelines.xsd">

    <!--
    Add xml elements to name your pipeline and chain processes together here
    Note that the interface will display these pipelines in declaration order, with the first pipeline as the default
    -->

    <!-- AE2 processes -->
    <pipeline name="Experiment Loading (Combined AE2/Atlas)" creator="natalja" daemonized="true">
        <processes>
            <process name="validation" />
            <process name="experiment loading" />
            <process name="scoring" />
            <process name="afterloading" />
            <process name="atlas validation" />
            <process name="atlas loading" />
        </processes>
    </pipeline>

    <pipeline name="Atlas Experiment Unloading" creator="natalja">
        <processes>
            <process name="atlas unloading" />
        </processes>
    </pipeline>

    <pipeline name="Atlas Experiment Private" creator="natalja">
        <processes>
            <process name="atlas private" />
        </processes>
    </pipeline>

    <pipeline name="Atlas Experiment Public" creator="natalja">
        <processes>
            <process name="atlas public" />
        </processes>
    </pipeline>

</conan-pipelines>