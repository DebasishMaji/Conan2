<?xml version="1.0" encoding="UTF-8"?>

<conan-pipelines xmlns="http://www.ebi.ac.uk/arrayexpress/conan/pipelines"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://www.ebi.ac.uk/arrayexpress/conan/pipelines
                                     http://www.ebi.ac.uk/arrayexpress/conan/pipelines.xsd">
    
    <pipeline name="BioSamples (other)" creator="faulcon">
        <processes>
            <process name="accessionsampletab" displayName="accession sampletab.pre.txt" />
            <process name="toloadsampletab" displayName="to sampletab.toload.txt" />
            <process name="toguixml" displayName="to GUI XML" />
            <process name="toftp" displayName="copy to FTP site" />
<!--             <process name="sampletabmyequivalents" displayName="to MyEquivalents" /> -->
<!--             <process name="sampletabrelationalloader" displayName="to relational DB" /> -->
        </processes>
    </pipeline>
    
    <pipeline name="BioSamples make public" creator="faulcon">
        <processes>
            <process name="toftp" displayName="copy to FTP site" />
            <process name="makepublic" displayName="make public in AGE db" />
        </processes>
    </pipeline>
    
    <pipeline name="BioSamples make non-public" creator="faulcon">
        <processes>
            <process name="removeftp" displayName="remove from FTP site" />
            <process name="makenonpublic" displayName="make non-public in AGE db" />
        </processes>
    </pipeline>
    
    <pipeline name="Test performance" creator="faulcon">
        <processes>
            <process name="delay" displayName="do nothing for 60 seconds" />
        </processes>
    </pipeline>
</conan-pipelines>